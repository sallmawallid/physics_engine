<Window
    x:Class="physicsEngine.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:physicsEngine"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="400"
    Height="400"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Canvas Name="MyCanvas" Grid.Row="1" />
        <ComboBox
            Name="changecolor"
            Canvas.Left="136"
            Canvas.Top="14"
            Canvas.Right="7"
            Width="100"
            HorizontalAlignment="Left"
            VerticalAlignment="Center"
            SelectedIndex="0"
            SelectionChanged="selectionChanged1">
            <ComboBoxItem Content="Red" />
            <ComboBoxItem Content="Cyan" />
            <ComboBoxItem Content="PeachPuff" />
            <ComboBoxItem Content="HotPink" />
        </ComboBox>
    </Grid>
</Window>
